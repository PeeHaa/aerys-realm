{
    "name": "peehaa/aerys-realm-ii",
    "description": "Text based adventure",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Pieter Hordijk",
            "email": "info@pieterhordijk.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.3",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "amphp/amp": "^2.1.1",
        "amphp/file": "^0.3.3",
        "amphp/http-server": "^1.0.1",
        "amphp/http-server-router": "^1.0.1",
        "amphp/http-server-static-content": "^1.0.3",
        "amphp/log": "^1.0.0",
        "amphp/parallel-functions": "^0.1.3",
        "amphp/postgres": "^1.0.2",
        "amphp/websocket-server": "^1.0.0",
        "rdlowrey/auryn": "^1.4.2",
        "robmorgan/phinx": "^0.10.6"
    },
    "require-dev": {
        "harmonyio/php-codesniffer-ruleset": "1.0.0-rc1",
        "harmonyio/phpunit-extension": "^1.0.0-rc1",
        "maglnet/composer-require-checker": "^1.1.0",
        "phpunit/phpunit": "^7.5.1",
        "slevomat/coding-standard": "^4.8.6",
        "squizlabs/php_codesniffer": "^3.4.0"
    },
    "autoload": {
        "psr-4": {
            "PeeHaa\\AerysRealmII\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PeeHaa\\AerysRealmIITest\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
